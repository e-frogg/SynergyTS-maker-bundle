parameters:
    level: 8
    bootstrapFiles:
         - ../../../vendor/autoload.php
    paths:
        - src/
    excludePaths:
        - vendor(?)
        - rector*.php
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        -
            identifier: treatPhpDocTypesAsCertain
        -
            identifier: function.alreadyNarrowedType
        -
            identifier: instanceof.alwaysTrue
        -
            identifier: trait.unused
